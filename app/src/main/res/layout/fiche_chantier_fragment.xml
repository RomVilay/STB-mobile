<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ficheChantier.FicheChantier" >

    <ScrollView
        android:layout_width="768dp"
        android:layout_height="1029dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="-4dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="293dp"
                android:layout_height="128dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="30dp"
                android:contentDescription="logoSTB"
                android:src="@drawable/logo_grand_format" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="770dp"
                android:layout_height="195dp">

                <Button
                    android:id="@+id/select"
                    android:layout_width="46dp"
                    android:layout_height="52dp"
                    android:text="+"
                    app:backgroundTint="#939393"
                    app:cornerRadius="60px"
                    app:layout_constraintEnd_toStartOf="@+id/textView6"
                    app:layout_constraintHorizontal_bias="0.537"
                    app:layout_constraintStart_toEndOf="@+id/spinner2"
                    tools:layout_editor_absoluteY="0dp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="232dp"
                    android:layout_height="46dp"
                    android:layout_marginEnd="88dp"
                    android:layout_marginRight="88dp"
                    android:background="@drawable/round_textview"
                    android:gravity="center"
                    android:text="Client"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Spinner
                    android:id="@+id/spinner2"
                    android:layout_width="286dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="64dp"
                    android:layout_marginLeft="64dp"
                    android:background="@drawable/round_textview"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/vehicule"
                    android:layout_width="157dp"
                    android:layout_height="51dp"
                    android:layout_marginStart="64dp"
                    android:layout_marginLeft="64dp"
                    android:layout_marginBottom="24dp"
                    android:background="@drawable/round_textview"
                    android:gravity="center"
                    android:text="vehicule"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/numero"
                    android:layout_width="158dp"
                    android:layout_height="52dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginBottom="24dp"
                    android:background="@drawable/round_textview"
                    android:gravity="center"
                    android:text="Numero"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/linearLayout2"
                    app:layout_constraintStart_toEndOf="@+id/vehicule"
                    app:layout_constraintTop_toBottomOf="@+id/contact"
                    app:layout_constraintVertical_bias="1.0" />

                <TextView
                    android:id="@+id/contact"
                    android:layout_width="232dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="64dp"
                    android:layout_marginLeft="64dp"
                    android:background="@drawable/round_textview"
                    android:gravity="center"
                    android:text="Contact"
                    app:layout_constraintBottom_toTopOf="@+id/vehicule"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner2"
                    app:layout_constraintVertical_bias="0.214" />

                <TextView
                    android:id="@+id/adresse"
                    android:layout_width="387dp"
                    android:layout_height="51dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="56dp"
                    android:layout_marginRight="56dp"
                    android:background="@drawable/round_textview"
                    android:gravity="center"
                    android:text="Adresse"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView6" />

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="309dp"
                    android:layout_height="53dp"
                    android:layout_marginEnd="56dp"
                    android:layout_marginRight="56dp"
                    android:background="@drawable/round_textview"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/adresse"
                    app:layout_constraintVertical_bias="0.41">

                    <EditText
                        android:id="@+id/DateArrivee"
                        android:layout_width="149dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:inputType="date" />

                    <EditText
                        android:id="@+id/DateDepart"
                        android:layout_width="165dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:inputType="date" />
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView15"
                android:layout_width="536dp"
                android:layout_height="97dp"
                android:layout_marginBottom="14dp"
                android:background="@drawable/round_textview"
                android:gravity="center"
                android:text="Matériel Nécessaire" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="532dp"
                android:layout_height="87dp"
                android:layout_gravity="center"
                android:layout_marginBottom="14dp"
                android:background="@drawable/round_textview"
                android:gravity="center"
                android:text="Objet de l'intervention" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="543dp"
                android:layout_height="119dp"
                android:layout_marginBottom="14dp"
                android:background="@drawable/round_textview"
                android:gravity="center"
                android:text="Observations" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="532dp"
                android:layout_height="100dp"
                android:background="@drawable/round_textview"
                android:gravity="center"
                android:text="Diagnostic" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="147dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="242dp"
                    android:layout_height="107dp"
                    android:layout_marginEnd="20px"
                    android:layout_marginRight="20px"
                    android:layout_marginBottom="18dp"
                    android:background="@drawable/round_textview"
                    android:text="Signature Client" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="271dp"
                    android:layout_height="104dp"
                    android:layout_marginBottom="18dp"
                    android:background="@drawable/round_textview"
                    android:text="Signature Technicien" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>